<div class="container">
	<div class="screen">
		<div class="screen__content">
			<form class="login">
				<div class="login__field">
					<i class="login__icon fas fa-user"></i>
					<input type="text" class="login__input" placeholder="Your Email">
				</div>
				<div class="login__field">
					<i class="login__icon fas fa-solid fa-envelope"></i>
					<input type="text" class="login__input" placeholder="Enter Your message">
